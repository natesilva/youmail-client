{
  "name": "@reallyuseful/youmail-client",
  "version": "2.1.0",
  "description": "A Node.js client for the YouMail Spam Caller API (Unofficial)",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "clean": "del-cli build",
    "build": "tsc",
    "prettier": "prettier --write \"{src,test}/**/*.?(ts|json)\"",
    "lint": "eslint --ext .ts .",
    "prepare": "npm run clean && npm run build",
    "test": "mocha ./src/**/*.spec.ts",
    "test:watch": "mocha ./src/**/*.spec.ts  -w --watch-extensions ts",
    "coverage": "nyc --reporter lcov --reporter text --extension .ts mocha ./src/**/*.spec.ts"
  },
  "keywords": [
    "youmail",
    "robocall",
    "spam",
    "phone call"
  ],
  "author": "Nate Silva <nate@natesilva.com>",
  "license": "MIT",
  "repository": "natesilva/youmail-client",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/google-libphonenumber": "^7.4.17",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.18",
    "@types/node-fetch": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "mocha": "^6.2.0",
    "np": "^5.1.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "testdouble": "^3.12.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "google-libphonenumber": "^3.2.5",
    "node-fetch": "^2.6.0"
  }
}
